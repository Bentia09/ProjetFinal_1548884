@model IEnumerable<Symphonie.Models.Professeur>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div style="display: inline-block; background-color: #E6E6FA; padding: 5px 10px; border-radius: 5px;">
    <a asp-action="Index" asp-controller="Orchestres">Retour</a>
</div>
<h1>Index</h1>



<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prenom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoEmployeChiffre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Instrument)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prenom)
            </td>
                <th>
                    @Html.DisplayFor(modelItem => item.NoEmployeChiffre)
                </th>
            <td>
                @Html.DisplayFor(modelItem => item.Instrument.InstrumentId)
            </td>
            <td>
               
                <a asp-action="Details" asp-route-id="@item.ProfesseurId">Details</a> 
                
            </td>
        </tr>
}
    </tbody>
</table>
