@model IEnumerable<Symphonie.Models.InstrumentVent>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TypeVent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Materiaux)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Instrument)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TypeVent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Materiaux)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Instrument.InstrumentId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InstrumentVentId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InstrumentVentId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InstrumentVentId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
